{
  "files.exclude": {},
  "workbench.colorCustomizations": {
    "activityBar.background": "#e66a54",
    "activityBar.foreground": "#000000",
    "statusBar.debuggingBackground": "#15d8ff",
    "statusBar.background": "#e66a54"
  },
  "editor.rulers": [
    80,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": false,
  "eslint.enable": true,
  "tslint.enable": false,
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    "tmp-*": true,
    "src/lib/env/**/*.*": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/tmp-*": true,
    "**/src/lib/env/**/*.*": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "eslint.migration.2_x": "off",
  "prettier.enable": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "tslint.alwaysShowRuleFailuresAsWarnings": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "prettier.endOfLine": "auto",
  "prettier.prettierPath": "./node_modules/prettier",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ],
  "window.title": "npm/taon-dev/taon  [ ${activeEditorShort} ]",
  "json.schemas": [
    {
      "fileMatch": [
        "/docs-config.jsonc"
      ],
      "url": "./docs-config.schema.json"
    },
    {
      "fileMatch": [
        "/taon.jsonc"
      ],
      "url": "./taon-config-standalone.schema.json"
    }
  ],
  "files.watcherExclude": {
    "local_release/**": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  }
}