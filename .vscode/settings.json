{
  "files.exclude": {
    "**/*____ORIGINAL____.ts": false,
    "_changelog": false,
    "*.sh": false,
    "*.xlsx": false,
    "scripts": false,
    "**/node_modules": false,
    "**/tmp*": false,
    "**/dist*": false,
    "**/browser": false,
    "**/websql": false,
    "**/websql*": false,
    "**/module*": false,
    "**/backup": false,
    "**/module": false,
    "**/www": false,
    "**/Thumbs.db": false,
    "**/.DS_Store": false,
    "**/**/.DS_Store": false,
    "**/npm-debug.log*": false,
    "**/.sass-cache": false,
    "**/.sourcemaps": false,
    "**/tsconfig.isomorphic.json": false,
    "**/tsconfig.isomorphic-flat-dist.json": false,
    "**/tsconfig.browser.json": false,
    "**/tsconfig.json": false,
    "**/tsconfig.backend.dist.json": false,
    "**/projects/tmp*": false,
    "**/.gitignore": false,
    "**/.npmignore": false,
    "**/.babelrc": false,
    "**/.npmrc": false,
    "**/.prettierignore": false,
    "**/.prettierrc": false,
    "**/.editorconfig": false,
    "**/devDependencies.json": false,
    "**/package.json": false,
    "**/logo.svg": false,
    ".firedev": false,
    ".taon": false,
    ".tnp": false,
    "**/*.filetemplate": false,
    "**/tsconfig.*": false,
    "**/tslint.*": false,
    "**/recent.json": false,
    "**/angular.json": false,
    "**/webpack*": false,
    "**/run-*": false,
    "**/run.*": false,
    "**/package-lock.json": false,
    "**/protractor.conf.js": false,
    "**/karma.conf.js": false,
    "**/eslint.config.js": false,
    "**/update-vscode-package-json.js": false,
    "**/.eslintrc.json": false,
    "**/docs-config.schema.json": false,
    "**/taon-config-standalone.schema.json": false,
    "**/taon-config-container.schema.json": false,
    "**/index.js": false,
    "**/index.d.ts": false,
    "**/index.js.map": false
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#e66a54",
    "activityBar.foreground": "#000000",
    "statusBar.debuggingBackground": "#15d8ff",
    "statusBar.background": "#e66a54"
  },
  "editor.rulers": [
    80,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": false,
  "eslint.enable": true,
  "tslint.enable": false,
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    "tmp-*": true,
    "src/lib/env/**/*.*": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/tmp-*": true,
    "**/src/lib/env/**/*.*": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "eslint.migration.2_x": "off",
  "prettier.enable": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "tslint.alwaysShowRuleFailuresAsWarnings": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "prettier.endOfLine": "auto",
  "prettier.prettierPath": "./node_modules/prettier",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ],
  "window.title": "npm/taon-dev/taon  [ ${activeEditorShort} ]",
  "json.schemas": [
    {
      "fileMatch": [
        "/docs-config.jsonc"
      ],
      "url": "./docs-config.schema.json"
    },
    {
      "fileMatch": [
        "/taon.jsonc"
      ],
      "url": "./taon-config-standalone.schema.json"
    }
  ],
  "files.watcherExclude": {
    "local_release/**": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  }
}