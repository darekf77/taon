{
  "files.exclude": {
    "**/*____ORIGINAL____.ts": true,
    "_changelog": true,
    "*.sh": true,
    "*.xlsx": true,
    "scripts": true,
    "**/node_modules": true,
    "**/tmp*": true,
    "**/dist*": true,
    "**/browser": true,
    "**/websql": true,
    "**/websql*": true,
    "**/module*": true,
    "**/backup": true,
    "**/module": true,
    "**/www": true,
    "**/Thumbs.db": true,
    "**/.DS_Store": true,
    "**/**/.DS_Store": true,
    "**/npm-debug.log*": true,
    "**/.sass-cache": true,
    "**/.sourcemaps": true,
    "**/projects/tmp*": true,
    "**/tsconfig.backend.dist.json": true,
    "**/.gitignore": true,
    "**/.npmignore": true,
    "**/.babelrc": true,
    "**/.npmrc": true,
    "**/.prettierignore": true,
    "**/.prettierrc": true,
    "**/.editorconfig": true,
    "**/devDependencies.json": true,
    "**/package.json": true,
    "**/logo.svg": true,
    "**/*.filetemplate": true,
    "**/tsconfig.*": true,
    "**/tslint.*": true,
    "**/recent.json": true,
    "**/angular.json": true,
    "**/webpack*": true,
    "**/run-*": true,
    "**/run.*": true,
    "**/package-lock.json": true,
    "**/protractor.conf.js": true,
    "**/karma.conf.js": true,
    "**/docs-config.schema.json": true,
    "**/taon-config-standalone.schema.json": true,
    "**/taon-config-container.schema.json": true,
    "**/index.js": true,
    "**/index.d.ts": true,
    "**/index.js.map": true,
    "recent.json": true,
    "angular.json": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#ce9243",
    "activityBar.foreground": "#313129"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": true,
  "tslint.enable": true,
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "window.title": "projects/container-v18/examples-v18  [ ${activeEditorShort} ]",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "/taon.jsonc"
      ],
      "url": "./taon-config-container.schema.json"
    }
  ]
}