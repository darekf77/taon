{
  "files.exclude": {
    "**/*____ORIGINAL____.ts": false,
    "_changelog": false,
    "webpack.backend-dist-build.js": false,
    "**/node_modules": false,
    "**/tmp*": false,
    "**/dist*": false,
    "**/browser": false,
    "**/websql": false,
    "**/websql*": false,
    "**/module*": false,
    "**/backup": false,
    "**/module": false,
    "**/www": false,
    "**/Thumbs.db": false,
    "**/.DS_Store": false,
    "**/**/.DS_Store": false,
    "**/npm-debug.log*": false,
    "**/.sass-cache": false,
    "**/.sourcemaps": false,
    "**/tsconfig.isomorphic.json": false,
    "**/tsconfig.isomorphic-flat-dist.json": false,
    "**/tsconfig.browser.json": false,
    "**/tsconfig.json": false,
    "**/tsconfig.backend.dist.json": false,
    "**/index.js": false,
    "**/index.d.ts": false,
    "**/index.js.map": false,
    "**/webpack.config.js": false,
    "**/webpack.backend-dist-build.js": false,
    "**/run.js": false,
    "**/run-org.js": false,
    "**/tsconfig.isomorphic.json.filetemplate": false,
    "**/tsconfig.isomorphic-flat-dist.json.filetemplate": false,
    "**/tsconfig.browser.json.filetemplate": false,
    "**/tsconfig.json.filetemplate": false,
    "**/tsconfig.backend.dist.json.filetemplate": false,
    "**/.gitignore": false,
    "**/.npmignore": false,
    "**/.babelrc": false,
    "**/.npmrc": false,
    "**/.eslintrc.json": false,
    "**/.prettierignore": false,
    "**/.prettierrc": false,
    "**/.editorconfig": false,
    "**/devDependencies.json": false,
    "**/package.json": false,
    "**/logo.svg": false,
    "**/*.filetemplate": false,
    "**/tsconfig.*": false,
    "**/tslint.*": false,
    "**/recent.json": false,
    "**/angular.json": false,
    "**/webpack*": false,
    "**/run-*": false,
    "**/run.*": false,
    "**/package-lock.json": false,
    "**/protractor.conf.js": false,
    "**/karma.conf.js": false,
    "**/docs-config.schema.json": false
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#3186d4"
  },
  "workbench.colorTheme": "Default Light+",
  "editor.rulers": [
    80,
    120
  ],
  "tslint.autoFixOnSave": true,
  "tslint.enable": false,
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "eslint.migration.2_x": "off",
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.endOfLine": "auto",
  "json.schemas": [
    {
      "fileMatch": [
        "/docs-config.jsonc"
      ],
      "url": "./docs-config.schema.json"
    }
  ],
  "window.title": "container-v18/examples-v18/single-file  [ ${activeEditorShort} ]",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ]
}
