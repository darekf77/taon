{
  "files.exclude": {
    "**/*____ORIGINAL____.ts": false,
    "_changelog": false,
    "out": false,
    "update-proj.js": false,
    ".vscodeignore": false,
    "*.vsix": false,
    "**/node_modules": false,
    "**/tmp*": false,
    "**/dist*": false,
    "**/browser": false,
    "**/websql": false,
    "**/websql*": false,
    "**/module*": false,
    "**/backup": false,
    "**/module": false,
    "**/www": false,
    "**/Thumbs.db": false,
    "**/.DS_Store": false,
    "**/**/.DS_Store": false,
    "**/npm-debug.log*": false,
    "**/.sass-cache": false,
    "**/.sourcemaps": false,
    "**/.vscode/tasks.json": false,
    "**/.vscode/launch.json": false,
    "**/.vscodeignore": false,
    "**/vsc-extension-quickstart.md": false,
    "**/tsconfig.json": false,
    "**/update-proj.js": false,
    "**/src/extension.ts": false,
    "**/src/helpers.ts": false,
    "**/src/models.ts": false,
    "**/src/execute-command.ts": false,
    "**/src/progress-output.ts": false,
    "**/tsconfig.backend.dist.json": false,
    "**/.gitignore": false,
    "**/.npmignore": false,
    "**/.babelrc": false,
    "**/.npmrc": false,
    "**/.eslintrc.json": false,
    "**/.prettierignore": false,
    "**/.prettierrc": false,
    "**/.editorconfig": false,
    "**/devDependencies.json": false,
    "**/package.json": false,
    "**/logo.svg": false,
    "**/*.filetemplate": false,
    "**/tsconfig.*": false,
    "**/tslint.*": false,
    "**/recent.json": false,
    "**/angular.json": false,
    "**/webpack*": false,
    "**/run-*": false,
    "**/run.*": false,
    "**/package-lock.json": false,
    "**/protractor.conf.js": false,
    "**/karma.conf.js": false,
    "**/docs-config.schema.json": false,
    "**/index.js": false,
    "**/index.d.ts": false,
    "**/index.js.map": false
  },
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "typescript.tsc.autoDetect": "off",
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "workbench.colorCustomizations": {
    "activityBar.background": "#2570b6",
    "statusBar.background": "#5c3735",
    "statusBar.debuggingBackground": "#5c3735"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "editor.rulers": [
    80,
    120
  ],
  "eslint.migration.2_x": "off",
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.endOfLine": "auto",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ],
  "window.title": "plugins-v18/taon-vscode-ext () [ ${activeEditorShort}]",
  "json.schemas": [
    {
      "fileMatch": [
        "/docs-config.jsonc"
      ],
      "url": "./docs-config.schema.json"
    }
  ]
}