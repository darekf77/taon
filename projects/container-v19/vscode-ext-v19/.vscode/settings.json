{
  "files.exclude": {
    "**/*____ORIGINAL____.ts": false,
    "_changelog": false,
    "out": false,
    "update-proj.js": false,
    ".vscodeignore": false,
    "*.vsix": false,
    "*.sh": false,
    "*.xlsx": false,
    "scripts": false,
    "**/node_modules": false,
    "**/tmp*": false,
    "**/dist*": false,
    "**/browser": false,
    "**/websql": false,
    "**/websql*": false,
    "**/module*": false,
    "**/backup": false,
    "**/module": false,
    "**/www": false,
    "**/Thumbs.db": false,
    "**/.DS_Store": false,
    "**/**/.DS_Store": false,
    "**/npm-debug.log*": false,
    "**/.sass-cache": false,
    "**/.sourcemaps": false,
    "**/src-for-stanalone": false,
    "**/components-for-stanalone": false,
    "**/projects/tmp*": false,
    "**/tsconfig.backend.dist.json": false,
    "**/.gitignore": false,
    "**/.npmignore": false,
    "**/.babelrc": false,
    "**/.npmrc": false,
    "**/.eslintrc.json": false,
    "**/.prettierignore": false,
    "**/.prettierrc": false,
    "**/.editorconfig": false,
    "**/devDependencies.json": false,
    "**/package.json": false,
    "**/logo.svg": false
  },
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "typescript.tsc.autoDetect": "off",
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "workbench.colorCustomizations": {
    "activityBar.background": "#25b6a3",
    "statusBar.background": "#25b6a3",
    "statusBar.debuggingBackground": "#25b6a3"
  },
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "editor.rulers": [
    80,
    120
  ],
  "eslint.migration.2_x": "off",
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.inlineSuggest.enabled": true,
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.endOfLine": "auto",
  "window.title": "projects/container-v18/vscode-ext-v18  [ ${activeEditorShort} ]",
  "json.schemas": [
    {
      "fileMatch": [
        "/docs-config.jsonc"
      ],
      "url": "./docs-config.schema.json"
    },
    {
      "fileMatch": [
        "/taon.jsonc"
      ],
      "url": "./taon-config.schema.json"
    }
  ]
}
