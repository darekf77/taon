{
  "files.exclude": {
    "node_modules": true,
    "tnp-system-path.txt": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "tslint.json": true,
    ".editorconfig": true,
    "tsconfig.json": true,
    "src/typings.d.ts": true,
    "src/main.ts": true,
    "src/polyfills.ts": true,
    "src/tsconfig.app.json.filetemplate": true,
    "src/tsconfig.spec.json": true,
    "protractor.conf.js": true,
    "karma.conf.js": true,
    "webpack.config.build.aot.js": true,
    "webpack.config.build.js": true,
    "webpack.config.common.js": true,
    "webpack.config.js": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#da5a5a",
    "activityBar.foreground": "#313129"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": true,
  "tslint.enable": true
}